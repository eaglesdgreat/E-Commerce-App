{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "eslint:recommended"
    ],
    "parser": "babel-eslint",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": 0,
        "react/jsx-filename-extension": 0,
        "quotes": 0,
        "react/prefer-stateless-function": 0,
        "import/no-useless-path-segments": 0,
        "no-console": 0,
        "prefer-template": 0,
        "import/no-extraneous-dependencies": 0,
        "func-names": 0,
        "no-underscore-dangle": 0,
        "no-useless-escape": 0,
        "object-shorthand": 0
    }
}
